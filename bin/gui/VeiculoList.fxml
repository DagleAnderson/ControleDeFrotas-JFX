<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="343.0" prefWidth="536.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.VeiculoListController">
   <children>
      <Label prefHeight="20.0" prefWidth="550.0" style="-fx-background-color: #ddd;" text="Veículo de Transporte">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label text="Pesquisar" />
      <TextField prefHeight="25.0" prefWidth="449.0" />
      <TableView fx:id="tableViewVeiculo" prefHeight="225.0" prefWidth="536.0">
        <columns>
          <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="Código" />
          <TableColumn fx:id="tableColumnDesc" prefWidth="75.0" text="Descrição" />
            <TableColumn fx:id="tableColumnPlaca" prefWidth="75.0" text="Placa" />
            <TableColumn fx:id="tableColumnRenavam" prefWidth="75.0" text="RENAVAM" />
            <TableColumn fx:id="tableColumnMod" prefWidth="75.0" text="Modelo" />
        </columns>
      </TableView>
      <ButtonBar prefHeight="43.0" prefWidth="536.0" style="-fx-background-color: #40E0D0;">
        <buttons>
            <Button fx:id="btnIncluir" mnemonicParsing="false" onAction="#onBtnIncluirAction" text="Incluir" />
          <Button fx:id="btnEditar" mnemonicParsing="false" onAction="#onBtnEditarAction" text="Editar" />
            <Button fx:id="btnExcluir" mnemonicParsing="false" onAction="#onBtnExcluirAction" text="Excluir" />
            <Button fx:id="btnSair" mnemonicParsing="false" onAction="#onBtnSairAction" text="Sair" />
        </buttons>
      </ButtonBar>
   </children>
</VBox>
